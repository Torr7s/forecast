{
  "name": "beach-forecast-api",
  "author": {
    "email": "torr7s.contact@gmail.com",
    "name": "Matheus Torres"
  },
  "description": "Api to calculate the best surfing condition among several beaches",
  "version": "1.0.0",
  "license": "MIT",
  "main": "index.js",
  "engines": {
    "node": ">=16.16.0",
    "yarn": "1.22.19"
  },
  "scripts": {
    "build": "tsc",
    "lint": "eslint ./src ./tests --ext .ts",
    "lint:fix": "eslint ./src ./tests --ext .ts --fix",
    "start": "yarn build && node dist/src/index.js",
    "start:dev": "dotenv -e .env -- tsnd --transpile-only --ignore-watch 'node_modules' --respawn 'src/index.ts' | pino-pretty -c",
    "test": "yarn test:unit && yarn test:func",
    "test:func": "dotenv -e .env -- jest --projects ./tests --runInBand",
    "test:unit": "dotenv -e .env -- jest"
  },
  "dependencies": {
    "@overnightjs/core": "^1.7.6",
    "axios": "^0.27.2",
    "bcrypt": "^5.0.1",
    "config": "^3.3.8",
    "cors": "^2.8.5",
    "dayjs": "^1.11.5",
    "express": "^4.18.1",
    "express-openapi-validator": "^4.13.8",
    "express-pino-logger": "^7.0.0",
    "express-rate-limit": "^6.6.0",
    "http-status-codes": "^2.2.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "module-alias": "^2.2.2",
    "mongoose": "^6.6.1",
    "multer": "^1.4.5-lts.1",
    "node-cache": "^5.1.2",
    "pino": "^8.6.0",
    "pino-pretty": "^9.1.0",
    "supertest": "^6.2.4",
    "swagger-ui-express": "^4.5.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/config": "^3.3.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-pino-logger": "^4.0.3",
    "@types/http-status-codes": "^1.2.0",
    "@types/jest": "^29.0.0",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/lodash": "^4.14.185",
    "@types/module-alias": "^2.0.1",
    "@types/multer": "^1.4.7",
    "@types/nock": "^11.1.0",
    "@types/node": "^18.7.16",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "dotenv-cli": "^6.0.0",
    "eslint": "^8.23.0",
    "jest": "^29.0.2",
    "nock": "^13.2.9",
    "ts-jest": "^29.0.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.8.3"
  }
}
